{
  "name": "@stability/platform-ui",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist && rimraf node_modules",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "build:types": "tsc --noEmit",
    "build": "yarn build:preview",
    "build:preview": "yarn build:types && vite build --mode preview",
    "build:production": "yarn build:types && vite build --mode production",
    "dev:types": "tsc --noEmit --watch",
    "dev": "vite"
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "buffer": "^6.0.3",
    "daisyui": "^2.51.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
    "tailwind-merge": "^1.8.1",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "autoprefixer": "^10.4.13",
    "eslint": "8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "rimraf": "^4.1.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.2.4",
    "tsx": "^3.12.2",
    "typescript": "4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-image-optimizer": "^1.1.4",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
